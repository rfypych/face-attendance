{
  "name": "face-attendance-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tensorflow/tfjs": "^3.18.0",
    "@tensorflow/tfjs-backend-wasm": "^3.18.0",
    "@tensorflow/tfjs-core": "^3.18.0",
    "@vladmandic/face-api": "^1.7.15",
    "axios": "^1.4.0",
    "chart.js": "^4.4.8",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.14.2",
    "react-scripts": "5.0.1",
    "react-webcam": "^7.2.0"
  },
  "devDependencies": {
    "eslint-config-react-app": "^7.0.1"
  },
  "scripts": {
    "start": "react-scripts start --disable-host-check",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "clean": "rm -rf node_modules && npm cache clean --force",
    "reinstall": "npm run clean && npm install"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000",
  "devServer": {
    "allowedHosts": [
      "all",
      ".ngrok.io"
    ]
  }
}
